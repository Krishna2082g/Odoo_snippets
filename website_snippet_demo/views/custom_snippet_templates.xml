        <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
            <t t-snippet="website_snippet_demo.custom_snippet" t-thumbnail="/website_snippet_demo/static/src/img/snippet_thumb.jpg" />
        </xpath>
    </template>
</odoo>
